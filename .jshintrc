//http://www.cnblogs.com/code/articles/4103070.html
{
  "bitwise" : false, //禁用位运算符(如^， ，&)
  "camelcase": true,  //使用驼峰命名(camelCase)或全大写下划线命名(UPPER_CASE)
  "curly": true,   // if和while等语句中使用{}来明确代码块
  "eqeqeq": true,   // 使用===和!==替代==和!=
  "freeze": true, //禁止复写原生对象(如Array, Date)的原型
  "indent": 4,  //代码缩进宽度（空格数）
  "newcap": false,  // 构造函数名首字母必须大写
  "quotmark": "single",
  "maxdepth": 3,  //代码块中可以嵌入{}的最大深度
  "maxstatements": 50,  //函数中最大语句数
  "maxlen": 180,  //一行中最大字符数
  "eqnull": true,  // 允许==null
  "funcscope": true,  //允许在控制体内定义变量而在外部使用
  "strict": true, //强制使用ES5的严格模式
  "undef": true,  //禁止使用不在全局变量列表中的未定义的变量
  "unused": true, //禁止定义变量却不使用
  "noarg" : false,  //禁止使用arguments.caller和arguments.callee
  "trailing" : true,  //禁止行尾空格
  "node": true  // {int} Max depth of nested blocks (within functions)
}
